    "build:icons": "npm run clean:generated && npm run generate:svg-icons && npm run generate:misc-icons",
    "clean:generated": "rimraf components/SvgIcons components/MiscIcons",
    "generate:svg-icons": "svgr --config-file svgr.svg-icons.config.js -- public/svg-icons",
    "generate:misc-icons": "svgr --config-file svgr.misc-icons.config.js -- public/misc-icons"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "motion": "^12.4.7",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-datepicker": "^8.1.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3",
    "@iconify/react": "^5.2.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
